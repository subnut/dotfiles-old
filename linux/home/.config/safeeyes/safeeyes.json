{
    "allow_postpone": true,
    "long_break_duration": 120,
    "long_break_interval": 20,
    "long_breaks": [
        {
            "name": "Walk for a while"
        },
        {
            "name": "Lean back at your seat and relax"
        }
    ],
    "meta": {
        "config_version": "6.0.2"
    },
    "persist_state": true,
    "plugins": [
        {
            "enabled": true,
            "id": "donotdisturb",
            "settings": {
                "skip_break_windows": "",
                "take_break_windows": "",
                "unfullscreen": true,
                "while_on_battery": false
            },
            "version": "0.0.2"
        },
        {
            "enabled": true,
            "id": "notification",
            "version": "0.0.1"
        },
        {
            "enabled": false,
            "id": "audiblealert",
            "settings": {
                "post_break_alert": true,
                "pre_break_alert": false
            },
            "version": "0.0.3"
        },
        {
            "enabled": true,
            "id": "trayicon",
            "settings": {
                "allow_disabling": true,
                "disable_options": [
                    {
                        "time": 30,
                        "unit": "minute"
                    },
                    {
                        "time": 1,
                        "unit": "hour"
                    },
                    {
                        "time": 2,
                        "unit": "hour"
                    },
                    {
                        "time": 3,
                        "unit": "hour"
                    }
                ],
                "show_time_in_tray": false
            },
            "version": "0.0.3"
        },
        {
            "enabled": true,
            "id": "smartpause",
            "settings": {
                "idle_time": 5,
                "interpret_idle_as_break": false,
                "postpone_if_active": false
            },
            "version": "0.0.3"
        },
        {
            "enabled": true,
            "id": "screensaver",
            "settings": {
                "command": "i3lock -c 00000040 -k --pass-media-keys --pass-screen-keys  --radius 180 --ring-width 20 --linecolor 00000000 --ringcolor=ffffff --keyhlcolor=000000 --insidecolor=ffffff --indicator --ringwrongcolor ff2134  --separatorcolor 00000000 --ringvercolor 008cf7 --insidevercolor 008cf7 --insidewrongcolor ff2134 --pass-power-keys --refresh-rate=0.5 --bshlcolor=ff2134",
                "min_seconds": 5.0
            },
            "version": "0.0.2"
        },
        {
            "enabled": false,
            "id": "healthstats",
            "settings": {
                "statistics_reset_interval": 24.0
            },
            "version": "0.0.2"
        },
        {
            "enabled": true,
            "id": "mediacontrol",
            "version": "0.0.1"
        }
    ],
    "postpone_duration": 5,
    "pre_break_warning_time": 30,
    "rpc_port": 7200,
    "short_break_duration": 8,
    "short_break_interval": 5,
    "short_breaks": [
        {
            "name": "Tightly close your eyes"
        },
        {
            "name": "Roll your eyes a few times to each side"
        },
        {
            "name": "Rotate your eyes in clockwise direction"
        },
        {
            "name": "Rotate your eyes in counterclockwise direction"
        },
        {
            "name": "Blink your eyes"
        },
        {
            "name": "Focus on a point in the far distance"
        },
        {
            "name": "Have some water"
        }
    ],
    "shortcut_disable_time": 0,
    "shortcut_postpone": 65,
    "shortcut_skip": 9,
    "strict_break": false,
    "use_rpc_server": true
}